{{> header}}
  <div class="left">
    <div class="main">
      <div class="logo">
        <p id="title">GABRIELE PIROVANO</p>
      </div>
      <div class="menu">

        <a href="/gallery/Audacity/{{firstAudacity}}"><p>Audacity</p></a>
        <a href="/gallery/Colore/{{firstColore}}"><p>Colore</p></a>
        <a href="/gallery/B&W/{{firstBandw}}"><p>B&W</p></a>
        <a href="/gallery/Cooling/{{firstCooling}}"><p>Cooling</p></a>
        <a href="/gallery/Kidz/{{firstKidz}}"><p>Kidz</p></a>
        <a href="/gallery/Meetmeat/{{firstMeetmeat}}"><p>Meet meat</p></a>
        <a href="/gallery/Muse/{{firstMuse}}"><p>Muse</p></a>
        <a href="/gallery/Squatinn/{{firstSquatinn}}"><p>Squatt Inn</p></a>
      </div>
      <div class="contact">
        <p>Contact</p>
      </div>
    </div>
  </div>
  <div class="right">
    <div class="main">

      <div class="title">
        <p>{{picName}}</p>
      </div>
      <div class="focus">
        <img src="{{path}}" alt="{{picName}}" />
      </div>
      <div class="controls">
        <p><a href="/gallery/{{galleryTitle}}/{{prevImg}}">&lt; prev</a> | <a href="/gallery/{{galleryTitle}}/{{nextImg}}">next &gt;</a></p>
      </div>


    </div>
  </div>
{{> footer}}
